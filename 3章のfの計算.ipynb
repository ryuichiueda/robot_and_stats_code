   "source": [
    "from fractions import Fraction\n",
    "\n",
    "def f(i,j, n=5):\n",
    "    if i==0:\n",
    "        return 1 if j==0 else 0\n",
    "    \n",
    "    return Fraction(100 - n + j, 100) * f(i-1, j) + Fraction(n - j + 1, 100) * f(i-1,j-1)"
   ]
