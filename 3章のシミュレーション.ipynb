   "source": [
    "import random\n",
    "\n",
    "def game(n):\n",
    "    return [random.randint(1,n) for _ in range(5) ]\n",
    "\n",
    "def score(g):\n",
    "    return len(list(filter(lambda n: n <= 5, set(g))))"
   ]
  },
  {
