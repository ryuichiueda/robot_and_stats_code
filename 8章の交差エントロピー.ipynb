   "outputs": [],
   "source": [
    "def cross_ent(vy1, vy2):\n",
    "    H = 0.0\n",
    "    for (y1, y2) in zip(vy1, vy2):\n",
    "        H -= y1 * np.log(y2)\n",
    "        \n",
    "    return H\n",
    "        "
   ]
  },
